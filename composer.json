{
  "name": "moderntribe/tribe-cli",
  "description": "A collection of WP-CLI utilities for testing and maintenance purposes.",
  "type": "wordpress-plugin",
  "license": "GPL-2.0",
  "authors": [
    {
      "name": "Modern Tribe, Inc.",
      "email": "tribe@tri.be"
    }
  ],
  "require": {
    "xrstf/composer-php52": "^1.0",
    "lucatume/di52": "~2.0",
    "fzaninotto/faker": "^1.7"
  },
  "autoload": {
    "files": ["autoload.php"]
  },
  "scripts": {
    "post-install-cmd": [
      "xrstf\\Composer52\\Generator::onPostInstallCmd"
    ],
    "post-update-cmd": [
      "xrstf\\Composer52\\Generator::onPostInstallCmd"
    ],
    "post-autoload-dump": [
      "xrstf\\Composer52\\Generator::onPostInstallCmd"
    ]
  }
}
